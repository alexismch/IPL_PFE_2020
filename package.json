{
  "name": "ipl_pfe_2020",
  "version": "0.0.1",
  "description": "Projet de Fin d'Études 2020 du Groupe 1 à l'Institut Paul Lambin",
  "dependencies": {
    "@types/express": "^4.17.9",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.14.10",
    "bcrypt": "^5.0.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.10.18",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.6",
    "router": "^1.3.5",
    "typescript": "^4.1.2"
  },
  "main": "index.js",
  "repository": "https://github.com/alexismch/IPL_PFE_2020.git",
  "author": "Michiels Alexis <pro@alexismch.be>",
  "license": "MIT",
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "_moduleAliases": {
    "@routes": "dist/routes",
    "@models": "dist/models"
  },
  "scripts": {
    "all": "concurrently \"npm start\" \"npm start --prefix front_end\"",
    "start": "concurrently \"tsc -w\" \"nodemon dist/app.js\"",
    "build": "npm run prebuild && npm run build:front && npm run build:back",
    "prebuild": "npm install --prefix front_end",
    "build:front": "npm run build --prefix front_end",
    "build:back": "tsc",
    "web": "node dist/js/app.js"
  }
}
